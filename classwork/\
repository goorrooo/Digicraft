#include <iostream>


class Tree{
public:
	Tree() {};

public:
	void insert(int val){
		*root = recursive(root,val);
		}

// Struct
	Node* recursive(Node* node, int val){
				if(node == nullptr){
			return new Node(val);
	}if(val < node->value){	
		node->left = recursive(node->left,val);
	} else{	
		node->right = recursive(node->right,val);
		}
	return node;
}
private:
	Node* root;


private:
struct Node{
public:
	Node(int val) : value(val), left(nullptr), right(nullptr){};
	int value;
	Node* left;
	Node* right;	
};

};

// Member
int main(){
	
	return 0;	
};
